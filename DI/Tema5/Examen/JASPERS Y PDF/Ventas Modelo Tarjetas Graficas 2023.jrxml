	<queryString language="SQL">
		<![CDATA[SELECT examen_tema5.articulos.`Descripcion`,
	examen_tema5.articulos.`Precio`,
	examen_tema5.detalle_pedidos.`Unidades`,
	examen_tema5.detalle_pedidos.`ID_Pedido`,
	examen_tema5.pedidos.`Fecha_Pedido`,
	examen_tema5.telefonos.`Telefono`,
	examen_tema5.emails.`Email`,
	examen_tema5.clientes.`Nombre` AS NombreCliente,
	examen_tema5.clientes.`Direccion`,
	examen_tema5.clientes.`Ciudad`,
	examen_tema5.clientes.`ID_Cliente`,
	examen_tema5.telefonos.`Persona_Contacto`,
	examen_tema5.detalle_pedidos.`Unidades` * examen_tema5.articulos.`Precio` AS PrecioXUnidades,
	'21%' AS IVA,
	examen_tema5.detalle_pedidos.`ID_Articulo`
FROM examen_tema5.detalle_pedidos
	INNER JOIN examen_tema5.articulos ON 
	 examen_tema5.detalle_pedidos.`ID_Articulo` = examen_tema5.articulos.`Id_articulo` 
	INNER JOIN examen_tema5.pedidos ON 
	 examen_tema5.detalle_pedidos.`ID_Pedido` = examen_tema5.pedidos.`ID_Pedido` 
	INNER JOIN examen_tema5.clientes ON 
	 examen_tema5.pedidos.`ID_Cliente` = examen_tema5.clientes.`ID_Cliente` 
	Inner Join examen_tema5.emails ON 
	 examen_tema5.emails.`Id_cliente` = examen_tema5.clientes.`ID_Cliente` 
	Inner Join examen_tema5.telefonos ON 
	 examen_tema5.telefonos.`Id_Cliente` = examen_tema5.clientes.`ID_Cliente` 
]]>
	</queryString>
