	<queryString language="SQL">
		<![CDATA[SELECT world.city.`Name` AS City_Name,
	world.country.`Code`,
	world.country.`Continent`,
	world.country.`Population`,
	world.countrylanguage.`IsOfficial`,
	world.country.`Name` AS Country_Name
FROM world.city
	INNER JOIN world.country ON 
	 world.city.`CountryCode` = world.country.`Code` 
	inner Join world.countrylanguage ON 
	 world.countrylanguage.`CountryCode` = world.country.`Code` 
WHERE 
	 world.countrylanguage.`IsOfficial` = 'NULL' 
	 AND world.countrylanguage.`Language` = $P{Filtro} 
ORDER BY world.country.`Continent` ASC ,
	world.country.`Code` ASC]]>
	</queryString>
